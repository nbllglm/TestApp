<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:orientation="vertical">

        <TextView
            android:id="@+id/startServiceA"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="startServiceA" />

        <TextView
            android:id="@+id/stopServiceA"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="stopServiceA" />

        <TextView
            android:id="@+id/startServiceB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="startServiceB" />

        <TextView
            android:id="@+id/stopServiceB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="stopServiceB" />

        <TextView
            android:id="@+id/bindServiceB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="bindServiceB" />

        <TextView
            android:id="@+id/unbindServiceB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="unbindServiceB" />

        <TextView
            android:id="@+id/bindServiceC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="bindServiceC" />

        <TextView
            android:id="@+id/unbindServiceC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="unbindServiceC" />


        <TextView
            android:id="@+id/sendCText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="sendCText" />

        <TextView
            android:id="@+id/responseCText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="" />


        <TextView
            android:id="@+id/startIntentService"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="startIntentService" />

        <TextView
            android:id="@+id/bindMessengerService"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="bindMessengerService" />

        <TextView
            android:id="@+id/unbindMessengerService"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="unbindMessengerService" />

        <TextView
            android:id="@+id/sendMessengerText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="sendMessengerText" />

        <TextView
            android:id="@+id/responseMessengerText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="" />


    </LinearLayout>

    <gz.example.common.view.CommonTitleBar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title_text="serviceActivity" />

</merge>